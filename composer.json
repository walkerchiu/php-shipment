{
    "name": "walkerchiu/shipment",
    "type": "library",
    "authors": [
        {
            "name":  "Walker Chiu",
            "email": "chenjen.chiou@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "walkerchiu/core": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WalkerChiu\\Shipment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WalkerChiu\\Shipment\\ShipmentServiceProvider"
            ],
            "aliases": {
                "Shipment": "WalkerChiu\\Shipment\\Models\\Entities\\Shipment",
                "ShipAny":  "WalkerChiu\\Shipment\\Models\\Entities\\ShipAny"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
